{
  "name": "Timer Extension",
  "description": "A Simple Timer Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "48": "img/logo-48.png",
    "128": "img/logo-128.png"
  },
  "action": {
    "default_icon": {
      "32": "img/icon.png"
    },
    "default_title": "Timer Chrome Extension",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "alarms"
  ],
  "background": {
    "service_worker": "background.js"
  }
}