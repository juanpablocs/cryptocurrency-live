{
  "name": "Binance Crypto ticker",
  "description": "Cryptocurrency live ticker Chrome Extension",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "48": "img/logo-128.png",
    "128": "img/logo-128.png"
  },
  "action": {
    "default_icon": {
      "64": "img/logo-128.png"
    },
    "default_popup": "index.html"
  },
  "permissions": [
    "storage",
    "alarms",
    "webRequest"
  ],
  "background": {
    "service_worker": "background.js"
  }
}